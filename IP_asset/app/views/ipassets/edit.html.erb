<h1>Editing ipasset</h1>
 
<%= form_for :ipasset, url: ipasset_path(@ipasset), method: :patch do |f| %>
 
  <% if @ipasset.errors.any? %>
    <div id="error_explanation">
      <h2>
        <%= pluralize(@ipasset.errors.count, "error") %> prohibited
        this ipasset from being saved:
      </h2>
      <ul>
        <% @ipasset.errors.full_messages.each do |msg| %>
          <li><%= msg %></li>
        <% end %>
      </ul>
    </div>
  <% end %>
 
<p>
    <%= f.label :title %><br>
    <%= f.text_field :title %>
  </p>
 
  <p>
    <%= f.label :description %><br>
    <%= f.text_area :description %>
  </p>
  
  <p>
    <%= f.label :binary_data %><br>
    <%= f.file_field :binary_data %>
  </p>

  <p>
    <%= f.label :filename %><br> 
    <%= f.text_field :filename %>
  </p>

 
  <p>
    <%= f.submit %>
  </p>
 
<% end %>
 
<%= link_to 'Back', ipassets_path %>
